<jsp:root xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" version="2.0">

  <jsp:output omit-xml-declaration="yes" />
  
  <spring:url value="/scripts/jquery.js" var="jquery" />
  <spring:url value="/scripts/jquery-ui.custom.min.js" var="jquery-ui" />
  <spring:url value="/scripts/dialogbox.js" var="dialogbox"/>
  <script src="${jquery}" type="text/javascript"><!-- required for FF3 and Opera --></script>
  <script src="${jquery-ui}" type="text/javascript"><!-- required for FF3 and Opera --></script>
  <script src="${dialogbox}" type="text/javascript"><!-- required for FF3 and Opera --></script>
</jsp:root>